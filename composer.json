{
    "name": "phpfastcache/phpfastcache-bundle",
    "type" : "symfony-bundle",
    "description": "PHP Bundle",
    "keywords": ["cache", "phpfastcache", "symfony", "php", "bundle"],
    "homepage": "http://www.phpfastcache.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
